{
    "name": "Jacana",
    "version": "1.0.0",
    "description": "water wastage extension",
    "manifest_version": 3,
    "author": "Sorcha Corrigan",
    "web_accessible_resources": ["images/*"],
    "action":{
        "default_popup": "index.html",
        "default_title": "Latest Covid Report",
        "js": ["script2.js"],
        "css": ["index.css"],
        "run_at": "document_start"
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content-ext.css"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "images2/*"
  ],
  "permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}